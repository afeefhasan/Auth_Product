		"ts-node": "^8.5.2",
		"typescript": "^3.7.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": [
			"prettier --write",
			"git add"
		]
	},
	"ava": {
		"compileEnhancements": false,
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	}

