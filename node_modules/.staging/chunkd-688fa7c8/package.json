	],
	"scripts": {
		"check:typescript": "tsc --noEmit",
		"check:prettier": "prettier --check '**'",
		"build": "rm -rf dist && tsc",
		"format": "prettier --write '**'",
		"test": "ava",
		"prepublishOnly": "npm run -s build"
	},
	"devDependencies": {
		"ava": "^2.4.0",
		"husky": "^3.1.0",
